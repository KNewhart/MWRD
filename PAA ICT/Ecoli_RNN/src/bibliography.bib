  author = {Stenekes, Nyree and Colebatch, Hal K. and Waite, T. David and Ashbolt, Nick J.},
  year = {2006},
  volume = {31},
  pages = {107--134},
  issn = {0162-2439},
  doi = {10.1177/0162243905283636},
  abstract = {ﾑPublic acceptanceﾒ is often seen as a key reason why water recycling technology is (accepted or) rejected. This paper examines the ways problems of water sustainability are defined by revisiting the notion of ﾑpublic acceptanceﾒ of water recycling. A common assumption is that projects fail because the general public is unable to comprehend specialist information about risk and the belief therefore, that if the public were better informed, they would accept change more readily. This is often assumed to be the reason for the failure of water recycling schemes, particularly those involving domestic and/or personal uses of the product water. This paper, in contrast, suggests that rhetoric about ﾑacceptanceﾒ is counterproductive in progressing sustainability as it does not address underlying issues relating to institutional arrangements and reinforces a dichotomy between expert and ﾑlayﾒ groups. Instead, institutional change is needed to build opportunities for constructive public engagement about deeply held values. The failure to implement sustainable water use through recycling can be understood as the result of several factors, including the present cost structures for water, institutional conservatism, administrative fragmentation and inadequate involvement of communities in planning. These factors constrain the definition of the problems and scope of acceptable solutions, resulting in the potential loss of economic and environmental opportunities to the community. Achieving sustainable water use through recycling may require better co-ordination between agencies and integrated government policies to overcome this complex and fragmented institutional framework.},
  journal = {Science Technology and Human Values},
  keywords = {Community engagement,Institutional approach,Public acceptance,Risk,Water recycling},
  note = {00101},
  number = {2}
}

